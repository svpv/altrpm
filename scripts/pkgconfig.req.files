#!/bin/sh -efu
[ -n "${RPM_LIBDIR-}" ] || RPM_LIBDIR=`rpm --eval %_libdir`
while IFS=$'\t' read -r f t; do
	case "$f" in
		${RPM_BUILD_ROOT-}/usr/share/pkgconfig/*.pc) ;;
		${RPM_BUILD_ROOT-}$RPM_LIBDIR/pkgconfig/*.pc) ;;
		*) continue ;;
	esac
	case "$t" in
		*" text"*) echo "$f" ;;
		*) echo "${0##*/}: $f: $t" >&2 ;;
	esac
done
